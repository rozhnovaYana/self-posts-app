{
  "expo": {
    "name": "Posts blog",
    "slug": "self-posts-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.yana.self_posts_app",
      "versionCode":1,
      "permissions":[
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
